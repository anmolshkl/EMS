CREATE CACHED TABLE "Secondary_Dets"("DepName" VARCHAR(50) NOT NULL,"DepRel" VARCHAR(50) NOT NULL,"Contact" INTEGER NOT NULL,"Email" VARCHAR(50) NOT NULL,"BloodGroup" VARCHAR(50) NOT NULL,"PanNum" VARCHAR(50),"LibNum" INTEGER,"EmployeeID" INTEGER NOT NULL)
SET TABLE "Primary_Dets" INDEX'4864 0'
