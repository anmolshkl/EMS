SET DATABASE COLLATION "Latin1_General"
CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE CACHED TABLE "Primary_Dets"("Addr" VARCHAR(50) NOT NULL,"FN" VARCHAR(50),"MN" VARCHAR(50) NOT NULL,"LN" VARCHAR(50) NOT NULL,"Dept" VARCHAR(50) NOT NULL,"MobNum" VARCHAR(30) NOT NULL,"Qualf" VARCHAR(50) NOT NULL,"Desig" VARCHAR(50) NOT NULL,"DOB" DATE NOT NULL,"EmployeeID" INTEGER NOT NULL,"DOJoining" DATE NOT NULL,"DOCreation" DATE NOT NULL,"DOMod" DATE NOT NULL,"ConfStat" BOOLEAN)
CREATE CACHED TABLE "Sal_Dets"("EmployeeID" INTEGER NOT NULL,"Designation" VARCHAR(50) NOT NULL,"Salary" INTEGER NOT NULL)
CREATE CACHED TABLE "Blckd_Pay"("EmployeeID" INTEGER NOT NULL,"FromDate" DATE NOT NULL,"ToDate" DATE NOT NULL,"Reason" VARCHAR(50) NOT NULL,"WorkflowID" INTEGER NOT NULL)
