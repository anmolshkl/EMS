CREATE CACHED TABLE "Login_Dets"("EmployeeID" INTEGER NOT NULL,"Password" VARCHAR(50) NOT NULL,"SecurityQuestion" VARCHAR(50) NOT NULL,"SecurityAnswer" VARCHAR(50) NOT NULL,"AccessLevel" INTEGER NOT NULL)
